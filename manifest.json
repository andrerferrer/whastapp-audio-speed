{
  "manifest_version": 2,
  "name": "Whatsapp audio speeder",
  "description": "a chrome extension to speed your audios created by andrerferrer",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
        "matches": [
          "https://web.whatsapp.com/"
        ],
        "css": [
          "style.css"
        ],
        "js": [
          "content.js"
        ]
    }
  ]
}


